<h1>New pwd_tracker</h1>

<% form_for(@pwd_tracker) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :urlKey %><br />
    <%= f.text_field :urlKey %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', pwd_trackers_path %>