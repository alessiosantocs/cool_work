<h1>New keyword_store</h1>

<% form_for(@keyword_store) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :keywords %><br />
    <%= f.text_area :keywords %>
  </p>
  <p>
    <%= f.label :company %><br />
    <%= f.text_field :company %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', keyword_stores_path %>