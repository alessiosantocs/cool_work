<h1>Listing personnels</h1>

<table>
  <tr>
    <th>First name</th>
    <th>Last name</th>
    <th>Current title</th>
    <th>Previous title</th>
    <th>Founder</th>
    <th>Grad edu</th>
    <th>Undergrad edu</th>
    <th>Other edu</th>
    <th>Private notes</th>
    <th>Company</th>
  </tr>

<% @personnels.each do |personnel| %>
  <tr>
    <td><%=h personnel.first_name %></td>
    <td><%=h personnel.last_name %></td>
    <td><%=h personnel.current_title %></td>
    <td><%=h personnel.previous_title %></td>
    <td><%=h personnel.founder %></td>
    <td><%=h personnel.grad_edu %></td>
    <td><%=h personnel.undergrad_edu %></td>
    <td><%=h personnel.other_edu %></td>
    <td><%=h personnel.private_notes %></td>
    <td><%=h personnel.company %></td>
    <td><%= link_to 'Show', personnel %></td>
    <td><%= link_to 'Edit', edit_personnel_path(personnel) %></td>
    <td><%= link_to 'Destroy', personnel, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New personnel', new_personnel_path %>