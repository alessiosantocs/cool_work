<h1>Listing draft_companies</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Url</th>
    <th>Founded</th>
    <th>Employee number</th>
    <th>Market segment</th>
    <th>Description</th>
    <th>Private notes</th>
    <th>Enabled</th>
    <th>Draft review</th>
  </tr>

<% @draft_companies.each do |draft_company| %>
  <tr>
    <td><%=h draft_company.name %></td>
    <td><%=h draft_company.url %></td>
    <td><%=h draft_company.founded %></td>
    <td><%=h draft_company.employee_number %></td>
    <td><%=h draft_company.market_segment %></td>
    <td><%=h draft_company.description %></td>
    <td><%=h draft_company.private_notes %></td>
    <td><%=h draft_company.enabled %></td>
    <td><%=h draft_company.draft_review %></td>
    <td><%= link_to 'Show', draft_company %></td>
    <td><%= link_to 'Edit', edit_draft_company_path(draft_company) %></td>
    <td><%= link_to 'Destroy', draft_company, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New draft_company', new_draft_company_path %>