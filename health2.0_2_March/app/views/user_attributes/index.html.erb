<h1>Listing user_attributes</h1>

<table>
  <tr>
    <th>Name</th>
    <th>User</th>
  </tr>

<% @user_attributes.each do |user_attribute| %>
  <tr>
    <td><%=h user_attribute.name %></td>
    <td><%=h user_attribute.User.login %></td>
    <td><%= link_to 'Destroy', user_attribute, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New user_attribute', new_user_attribute_path %>
